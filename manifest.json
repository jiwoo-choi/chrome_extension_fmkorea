{
    "name": "fmkorea-filter",
    "version": "0.6",
    "permissions": [
        "tabs",
        "<all_urls>",
        "storage"
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.fmkorea.com/*"
            ],
            "js": ["content.js"], 
            "run_at": "document_end"       
        }
    ], 

    "background" : {
        "scripts": ["background.js"],
        "persistent": false
    },
    
    "web_accessible_resources": [
        "background.js",
        "contentMain.js",
        "StorageController.js"
    ],
    "manifest_version":2
}